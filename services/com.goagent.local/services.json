{
    "id": "com.goagent.local.service",
    "description": "GoAgent Service",
    "services": [
        {
            "name": "com.goagent.local.service",
            "description": "goagent service for start/stop goagent",
            "commands": [
                {
                    "name": "status",
                    "assistant": "StatusAssistant",
                    "public": true
                },
                {
                    "name": "version",
                    "assistant": "VersionAssistant",
                    "public": true
                },
                {
                    "name": "start",
                    "assistant": "StartAssistant",
                    "public": true
                },
                {
                    "name": "stopProxy",
                    "assistant": "StopAssistant",
                    "public": true
                }
            ]
        }
    ]
}
